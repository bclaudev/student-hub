{
  "name": "student-hub",
  "version": "1.0.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "tailwind:watch": "npx tailwindcss -i ./src/style.css -o ./dist/output.css --watch",
    "server": "nodemon backend/index.js",
    "client": "react-scripts start",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run tailwind:watch\""
  },
  "type": "module",
  "main": "index.js",
  "dependencies": {
    "@react-oauth/google": "^0.12.1",
    "dotenv": "^16.4.5",
    "react-date-range": "^2.0.1",
    "react-feather": "^2.0.10",
    "react-router-dom": "^6.27.0",
    "react-scripts": "^5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "tailwindcss": "^3.4.14"
  }
}
